# Install trash_cli and use `trash` instead
alias rm='echo "This is not the command you are looking for."; false'

# Uses tree - install first:
# $ brew install tree
alias t='tree -I ".gradle|.idea|.vscode|.git|node_modules|bower_components|.DS_Store" --dirsfirst --filelimit 100 -L ${1:-3} -aC $2'

alias rn='react-native'
